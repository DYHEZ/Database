{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ],
  "crons": [
    {
      "path": "/api/ping",
      "schedule": "*/5 * * * *"
    },
    {
      "path": "/api/wakeup",
      "schedule": "*/10 * * * *"
    }
  ]
}